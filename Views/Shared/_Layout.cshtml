<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/admin-css")
    @Scripts.Render("~/bundles/angular")   
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">AngularProject</a>
            </div>
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Tests", "TestManagement", "Admin")</li>
                <li>@Html.ActionLink("Test's URL", "TestingUrlManagement", "Admin")</li>
                <li>@Html.ActionLink("Results", "ResultManagement", "Admin")</li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li>@Html.ActionLink("Log Out", "LogOff", "Account")</li>
            </ul>
        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p style="text-align:center">&copy; @DateTime.Now.Year - AngularProject</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    
    @Scripts.Render("~/bundles/angular-ui")
    @Scripts.Render("~/bundles/material-bootstrap-js")
    
</body>
</html>
